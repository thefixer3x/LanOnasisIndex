{
  "name": "LanOnasis-index",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Lan Onasis - Africa's leading AI-powered tech hub offering intelligent compliance, risk, and B2B payment solutions",
  "keywords": [
    "fintech",
    "AI",
    "compliance",
    "payments",
    "african-tech",
    "business-intelligence",
    "risk-management"
  ],
  "author": "Lan Onasis",
  "license": "MIT",
  "homepage": "https://LanOnasis.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/thefixer3x/LanOnasisIndex.git"
  },
  "scripts": {
    "dev": "bun vite",
    "build": "bun vite build",
    "lint": "bun eslint .",
    "preview": "bun vite preview",
    "type-check": "tsc --noEmit",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:translate": "npx lingo.dev@latest i18n",
    "i18n:coverage": "cd ../.. && bun run i18n:coverage",
    "i18n:quality": "cd ../.. && bun run i18n:quality",
    "i18n": "npx lingo.dev@latest i18n",
    "i18n:check": "test -f locales/es.json && echo '✅ Translations exist' || echo '❌ Missing translations'",
    "i18n:generate": "node scripts/generate-missing-translations.js",
    "i18n:status": "node scripts/translate-content.js",
    "i18n:fix": "bun run i18n:generate && bun run i18n:validate"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.23.16",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.7.3",
    "react-intersection-observer": "^9.16.0",
    "three": "^0.180.0",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "lingo.dev": "^0.111.8",
    "postcss": "^8.5.6",
    "tailwindcss": "4.1.13",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
